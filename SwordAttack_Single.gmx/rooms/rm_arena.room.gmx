  <code>/// Character Creation, Target Assignments, Input Devices

// create player objects and assign variables



player1 = instance_create(256, 256, obj_player);

enemies = ds_list_create();
// IF ROOM CHANGES, DESTROY enemies

ds_list_add(enemies, instance_create(600, 200, obj_enemyDUMB));
ds_list_add(enemies, instance_create(500, 250, obj_enemyDUMB));
ds_list_add(enemies, instance_create(600, 300, obj_enemyDUMB));
ds_list_add(enemies, instance_create(100, 900, obj_enemyDUMB));
ds_list_add(enemies, instance_create(300, 900, obj_enemyDUMB));

num = ds_list_size(enemies);
for (i = 0; i &lt; num; i++) {
    enemy = ds_list_find_value(enemies, i);
    enemy.targetID = player1;
}


// can we do gamepads?  assign input devices
// make this safer
player1.deviceID = 0;

// player2.deviceID = 1;


// play background music
//audio_play_sound(snd_faust, 2, true);

control = instance_create(-50, -50, obj_control);
control.player1 = player1;
control.enemies = enemies;



player1.controlID = control;
//player2.controlID = control;


stats = instance_create(0,0,obj_stats);
stats.player1 = player1;
stats.enemies = enemies;

player1.statsID = stats;

//set controller deadzones
if gamepad_is_connected(0) gamepad_set_axis_deadzone(0, 0.05);
if gamepad_is_connected(1) gamepad_set_axis_deadzone(1, 0.05);</code>
